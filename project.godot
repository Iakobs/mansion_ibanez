[application]

config/name="The Ibanez Mansion"
config/description="A first person 3d game on exploring a house in the search of secrets."
run/main_scene="res://scenes/ui/menus/main_menu.tscn"
config/icon="res://icon.png"
config/name_es="La Mansión Ibáñez"
config/name_ca="La Mansió Ibáñez"

[autoload]

InputMapManager="*res://scenes/global/input_map_manager.tscn"
